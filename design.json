    "definitions": {
        "DatasetInfo": {
            "type": "object",
            "properties": {
                "name": {
                    "type": "string",
                    "description": "The name of the dataset"
                },
                "categories": {
                    "type": "array",
                    "items": {
                        "type": "string",
                        "enum": [
                            "business",
                            "crime",
                            "demography",
                            "education",
                            "employment",
                            "energy",
                            "environment",
                            "geo",
                            "health",
                            "housing",
                            "location",
                            "media",
                            "public sector",
                            "sensor",
                            "social media",
                            "statistics",
                            "transport",
                            "water"
                        ]
                    },
                    "minItems": 1,
                    "uniqueItems": true
                },
                "tags": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    },
                    "uniqueItems": true
                },
                "formats": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                },
                "ownership": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                },
                "sourceWebsite": {
                    "type": "string"
                },
                "sourceName": {
                    "type": "string"
                },
                "pricePlan": {
                    "type": "string",
                    "enum": [
                        "Free"
                    ]
                },
                "licenses": {
                    "type": "array",
                    "items": {
                        "type": "string",
                        "enum": [
                            "flickr-apis-terms-of-use",
                            "terms-and-conditions-for-information-and-services-at-food-gov-ukratings",
                            "odc-database-contents-license-dbcl-1-0",
                            "netatmo-api-terms-of-use",
                            "other",
                            "open-database-license-odbl-v1-0",
                            "os-open-data-license",
                            "creative-commons-attribution-license",
                            "open-government-license"
                        ]
                    },
                    "minItems": 1
                },
                "statement": {
                    "type": "string"
                }
            }
        },
        "DatasetStream": {
            "type": "object",
            "properties": {
                "type": {
                    "type": "string",
                    "description": "Stream datatype",
                    "enum": [
                        "Sensor",
                        "Event"
                    ]
                },
                "id": {
                    "type": "string",
                    "description": "The Feed id",
                    "readOnly": true
                },
                "previewUrl": {
                    "type": "string",
                    "description": "Link to the preview feed page in the data hub portal"
                },
                "feedUrl": {
                    "type": "string",
                    "description": "Feed API url to be used to write/read streams."
                },
                "key": {
                    "type": "string",
                    "description": "Key (label) used to write to the stream API. If the key name does not match an existing API key, a new one is created and bound to the given label."
                },
                "location": {
                    "type": "string",
                    "description": "Stream location mode.",
                    "enum": [
                        "Fixed",
                        "Mobile"
                    ]
                },
                "latitute": {
                    "type": "string",
                    "format": "float",
                    "description": "Source location latitude.",
                    "maximum": 90,
                    "minimum": -90
                },
                "longitude": {
                    "type": "string",
                    "format": "float",
                    "description": "Source location longitude.",
                    "maximum": 180,
                    "minimum": -180
                },
                "elevation": {
                    "type": "integer",
                    "format": "int32",
                    "description": "Source location elevation."
                },
                "postcode": {
                    "type": "string",
                    "description": "Postcode"
                },
                "datastreams": {
                    "type": "array",
                    "minItems": 1,
                    "items": {
                        "$ref": "#/definitions/Stream"
                    }
                }
            }
        },
        "Stream": {
            "type": "object",
            "properties": {
                "id": {
                    "type": "string",
                    "description": "Stream ID"
                },
                "title": {
                    "type": "string",
                    "description": "Stream title"
                },
                "unitType": {
                    "type": "string",
                    "description": "Stream Unit Type",
                    "enum": [
                        "Basic SI",
                        "Derived SI",
                        "Conversion Based Units",
                        "Derived Units",
                        "Context Dependend Units"
                    ]
                },
                "unitName": {
                    "type": "string",
                    "description": "Stream Unit Name"
                },
                "unitSymbol": {
                    "type": "string",
                    "description": "Stream Unit Symbol"
                },
                "maximum": {
                    "type": "string",
                    "description": "Stream maximum value"
                },
                "minimum": {
                    "type": "string",
                    "description": "Stream minimum value"
                }
            }
        },
        "Access": {
            "type": "object",
            "properties": {
                "provider": {
                    "type": "string"
                },
                "everyone": {
                    "$ref": "#/definitions/AccessPolicy"
                },
                "exceptions": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/AccessPolicyException"
                    }
                }
            }
        },
        "AccessPolicy": {
            "type": "object",
            "properties": {
                "view": {
                    "type": "boolean",
                    "default": false
                },
                "subscribe": {
                    "type": "boolean",
                    "default": false
                }
            }
        },
        "AccessPolicyException": {
            "type": "object",
            "properties": {
                "user": {
                    "type": "string"
                },
                "view": {
                    "type": "boolean",
                    "default": false
                },
                "subscribe": {
                    "type": "boolean",
                    "default": false
                }
            },
            "required": [
                "user"
            ]
        },
        "Dataset": {
            "type": "object",
            "properties": {
                "id": {
                    "type": "string",
                    "description": "Dataset UUID"
                },
                "type": {
                    "type": "string",
                    "description": "The type of dataset",
                    "enum": [
                        "stream",
                        "file"
                    ]
                },
                "lastModified": {
                    "type": "string",
                    "format": "date-time",
                    "readOnly": true
                },
                "created": {
                    "type": "string",
                    "format": "date-time",
                    "readOnly": true
                },
                "pageUrl": {
                    "type": "string",
                    "description": "The web page of the dataset",
                    "readOnly": true
                },
                "apiUrl": {
                    "type": "string",
                    "description": "The url to the dataset json description",
                    "readOnly": true
                },
                "info": {
                    "type": "object",
                    "$ref": "#/definitions/DatasetInfo"
                },
                "stream": {
                    "type": "object",
                    "$ref": "#/definitions/DatasetStream"
                },
                "access": {
                    "type": "object",
                    "$ref": "#/definitions/Access"
                }
            },
            "required": [
                "id"
            ]
        },
        "License": {
            "type": "object",
            "properties": {
