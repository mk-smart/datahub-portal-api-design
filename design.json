{
    "swagger": "2.0",
    "info": {
        "description": "This is ongoing work for the MK:Smart Data Hub API.  You can find out more about MK:Smart at [http://mksmart.org](http://mksmart.org)",
        "version": "1.0.0",
        "title": "MK Data Hub API",
        "termsOfService": "http://datahub.mksmart.org",
        "contact": {
            "email": "enrico.daga@open.ac.uk"
        },
        "license": {
            "name": "Apache 2.0",
            "url": "http://www.apache.org/licenses/LICENSE-2.0.html"
        }
    },
    "host": "datahub.mksmart.org",
    "basePath": "/api/1.0",
    "tags": [
        {
            "name": "Data Catalogue",
            "description": "About datasets",
            "externalDocs": {
                "description": "List the datasets a user can view. Access dataset metadata.",
                "url": "#"
            }
        },
        {
            "name": "Data Sources",
            "description": "List the data sources and details about them.",
            "externalDocs": {
                "description": "Find out more about this API",
                "url": "#"
            }
        },
        {
            "name": "Subscriptions",
            "description": "Operations about user",
            "externalDocs": {
                "description": "Find out more about this API",
                "url": "#"
            }
        }
    ],
    "schemes": [
        "https"
    ],
    "paths": {
        "/datasets": {
            "get": {
                "tags": [
                    "Data Catalogue"
                ],
                "summary": "List datasets",
                "description": "List the datasets",
                "operationId": "catalogueListDatasets",
                "consumes": [
                    "*/*"
                ],
                "produces": [
                    "application/json"
                ],
                "parameters": [
                    {
                        "in": "query",
                        "type": "string",
                        "name": "fields",
                        "default": "id,title,url"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Returns the list of datasets",
                        "schema": {
                            "type": "array",
                            "items": {
                                "$ref": "#/definitions/Dataset"
                            }
                        }
                    },
                    "405": {
                        "description": "Invalid input",
                        "schema": {
                            "$ref": "#/definitions/ApiResponse"
                        }
                    }
                },
                "security": []
            }
        },
        "/dataset/{id}": {
            "get": {
                "tags": [
                    "Data Catalogue"
                ],
                "summary": "Find dataset by ID",
                "description": "Returns a single dataset",
                "operationId": "getDatasetById",
                "produces": [
                    "application/json"
                ],
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "description": "ID of dataset to return",
                        "required": true,
                        "type": "string"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "successful operation",
                        "schema": {
                            "$ref": "#/definitions/Dataset"
                        }
                    },
                    "400": {
                        "description": "Invalid ID supplied"
                    },
                    "404": {
                        "description": "Dataset not found"
                    }
                },
                "security": [
                    {
                        "api_key": []
                    }
                ]
            },
            "put": {
                "tags": [
                    "Data Catalogue"
                ],
                "summary": "Creates a dataset in the catalogue",
                "description": "",
                "operationId": "updateDatasetWithForm",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "description": "Dataset id",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "name": "dataset",
                        "in": "body",
                        "description": "Dataset",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/Dataset"
                        }
                    }
                ],
                "responses": {
                    "201": {
                        "description": "Dataset created,",
                        "schema": {
                            "$ref": "#/definitions/ApiResponse"
                        }
                    },
                    "406": {
                        "description": "The dataset already exists,",
                        "schema": {
                            "$ref": "/#definitions/ApiResponse"
                        }
                    },
                    "500": {
                        "description": "Server Error",
                        "schema": {
                            "$ref": "#/definitions/ApiResponse"
                        }
                    }
                },
                "security": []
            },
            "delete": {
                "tags": [
                    "Data Catalogue"
                ],
                "summary": "Deletes a dataset",
                "description": "",
                "operationId": "deleteDataset",
                "produces": [
                    "application/json"
                ],
                "parameters": [
                    {
                        "name": "api_key",
                        "in": "header",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "name": "id",
                        "in": "path",
                        "description": "Dataset id to delete",
                        "required": true,
                        "type": "string"
                    }
                ],
                "responses": {
                    "204": {
                        "description": "Dataset deleted",
                        "schema": {
                            "$ref": "#/definitions/ApiResponse"
                        }
                    },
                    "400": {
                        "description": "Invalid dataset value",
                        "schema": {
                            "$ref": "#/definitions/ApiResponse"
                        }
                    },
                    "500": {
                        "description": "Server Error",
                        "schema": {
                            "$ref": "#/definitions/ApiResponse"
                        }
                    }
                },
                "security": []
            }
        },
        "/datasources": {
            "get": {
                "tags": [
                    "Data Sources"
                ],
                "summary": "List my data sources",
                "description": "Provides a list of the data sources.",
                "operationId": "datasourcesList",
                "produces": [
                    "application/json"
                ],
                "parameters": [],
                "responses": {
                    "200": {
                        "description": "successful operation",
                        "schema": {
                            "type": "array",
                            "items": {
                                "$ref": "#/definitions/Datasource"
                            }
                        }
                    },
                    "500": {
                        "description": "Server Error",
                        "schema": {
                            "$ref": "#/definitions/ApiResponse"
                        }
                    }
                }
            }
        },
        "/subscriptions": {
            "get": {
                "tags": [
                    "Subscriptions"
                ],
                "summary": "List my subscriptions",
                "description": "Provides a list of the subscriptions.",
                "operationId": "subscriptionsList",
                "produces": [
                    "application/json"
                ],
                "parameters": [],
                "responses": {
                    "200": {
                        "description": "successful operation",
                        "schema": {
                            "type": "array",
                            "items": {
                                "$ref": "#/definitions/Subscription"
                            }
                        }
                    }
                }
            }
        }
    },
    "securityDefinitions": {
        "api_key": {
            "type": "apiKey",
            "name": "api_key",
            "in": "header"
        }
    },
    "definitions": {
        "Dataset": {
            "type": "object",
            "properties": {
                "id": {
                    "type": "string",
                    "description": "Dataset UUID"
                },
                "name": {
                    "type": "string",
                    "description": "The name of the dataset"
                },
                "type": {
                    "type": "string",
                    "description": "The type of dataset",
                    "enum": [
                        "stream",
                        "file"
                    ]
                },
                "page": {
                    "type": "string"
                },
                "description": {
                    "type": "string"
                },
                "lastModified": {
                    "type": "string",
                    "format": "date-time",
                    "readOnly": true
                },
                "created": {
                    "type": "string",
                    "format": "date-time",
                    "readOnly": true
                },
                "categories": {
                    "type": "array",
                    "items": {
                        "type": "string",
                        "enum": [
                            "business",
                            "crime",
                            "demography",
                            "education",
                            "employment",
                            "energy",
                            "environment",
                            "geo",
                            "health",
                            "housing",
                            "location",
                            "media",
                            "public sector",
                            "sensor",
                            "social media",
                            "statistics",
                            "transport",
                            "water"
                        ]
                    },
                    "minItems": 1,
                    "uniqueItems": true
                },
                "tags": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    },
                    "uniqueItems": true
                },
                "formats": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                },
                "policies": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/RedistributionPolicy"
                    }
                }
            },
            "required": [
                "id"
            ]
        },
        "RedistributionPolicy": {
            "type": "object",
            "properties": {
                "policyId": {
                    "type": "string",
                    "readOnly": true
                },
                "title": {
                    "type": "string"
                },
                "description": {
                    "type": "string"
                }
            },
            "required": [
                "policyId"
            ]
        },
        "Subscription": {
            "type": "object",
            "properties": {
                "key": {
                    "type": "string"
                },
                "id": {
                    "type": "string"
                },
                "active": {
                    "type": "boolean"
                }
            },
            "required": [
                "key",
                "id",
                "active"
            ]
        },
        "Datasource": {
            "type": "object",
            "properties": {
                "id": {
                    "type": "string"
                },
                "url": {
                    "type": "string"
                }
            },
            "required": [
                "id",
                "url"
            ]
        },
        "ApiResponse": {
            "type": "object",
            "properties": {
                "code": {
                    "type": "integer",
                    "format": "int32"
                },
                "type": {
                    "type": "string"
                },
                "message": {
                    "type": "string"
                }
            }
        }
    },
    "externalDocs": {
        "description": "Find out more about MK:Smart Datahub API",
        "url": "http://datahub.mksmart.org"
    }
}
